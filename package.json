{
  "name": "mev-boost-aa",
  "version": "1.0.0",
  "repository": "https://github.com/doublespending/MEVBoostAA",
  "author": "doublespending",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=16.0.0 <18",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "setup": "yarn install --frozen-lockfile",
    "format": "prettier --write .",
    "check-pretty": "prettier --check .",
    "lint": "solhint \"contracts/**/*.sol\"",
    "compile": "forge build --force",
    "test-foundry-local": "forge test --no-match-path 'test/forkMainnet/*.t.sol'",
    "test-foundry-fork": "forge test --fork-url $MAINNET_NODE_RPC_URL --fork-block-number 16776718 --match-path 'test/forkMainnet/*.t.sol'"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5"
  },
  "dependencies": {}
}
